{
    "$schema": "https://json-schema.org/draft/2020-12/schema",

    "title": "Event",
    "description": "Object which stores information about completed and future events.",
    "type": "object",
    "properties": {
        "begindate":{
            "description" : "Date & time when event begins/began",
            "type" : "string",
            "format":"date-time"
        },
        "eventid":{
            "description" : "Event unique identifier. Unsigned integer",
            "type" : "integer" 
        },
        "estimationdate":{
            "description" : "Date & time when event will be completed/was completed",
            "type" : "string",
            "format":"date-time"
        },
        "estimationdate":{
            "description" : "Date & time when event will be completed/was completed",
            "type" : "string",
            "format":"date-time"
        }
    }
}